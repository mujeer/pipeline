    pipeline {
  environment {
    registry = "mujeer472/nginx"
    registryCredential = 'Docker-Hub'
  }
  agent any
  stages {
    stage('Building image') {
      steps{
        script {
          docker.build registry + ":$BUILD_NUMBER"
          docker.push registry + ":$BUILD_NUMBER"
      
        }
      }
    }
  }
}
